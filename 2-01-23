#include<stdio.h>
#include<string.h>
void memcpyfun(void *dest,void *src,int len)
{
    int *s=(int*)src;
    int *d=(int*)dest;
    for(int i=0;i<len;i++)
    {
        d[i]=s[i];
    }
    for(int i=0;i<len;i++)
    printf("%d",d[i]);
}
int main()
{
    int s[]={1,2,3,4,5};
    int d[10];
    int len=sizeof(s)/sizeof(s[0]);
    memcpyfun(d,s,len);
}
